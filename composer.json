{
    "name": "ixudra/portfolio",
    "description": "Custom PHP Portfolio library for the Laravel 5 framework - developed by Ixudra",
    "version": "2.0.1",
    "keywords": ["Ixudra", "Laravel", "Portfolio", "Project"],
    "homepage": "http://ixudra.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@gmail.com"
        }
    ],

    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "~6.0",
        "laravelcollective/html": "~6.0",
        "ixudra/addressable": "~2.0",
        "ixudra/core": "~6.0",
        "ixudra/translation": "~5.1",
        "ixudra/imageable": "~8.0",
        "laracasts/presenter": "0.2.*"
    },

    "autoload": {
        "classmap": [
            "src/database/migrations"
        ],
        "psr-4": {
            "Ixudra\\Portfolio\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Ixudra\\Portfolio\\PortfolioServiceProvider"
            ],
            "aliases": {
                "Portfolio": "Ixudra\\Portfolio\\Portfolio"
            }
        }
    }
}
